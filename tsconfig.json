{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "allowJs": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "inlineSourceMap": false,
    "skipLibCheck": true,
    "lib": [
      "dom",
      "es2015",
      "es2016",
      "es2017",
      "es2018",
      "esnext"
    ],
    "jsx": "react",
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "noImplicitAny": false
  }
}